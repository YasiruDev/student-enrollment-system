{
  "name": "student-enrolllment-service",
  "version": "1.0.0",
  "description": "",
  "author": "Yasiru",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build-prod": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "dev": "nodemon",
    "qa": "cross-env NODE_ENV=qa nodemon",
    "start:dev": "node index.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage --runInBand"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^8.2.1",
    "@nestjs/common": "8.0.0",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "8.0.0",
    "@nestjs/platform-express": "8.0.0",
    "@nestjs/swagger": "5.2.0",
    "@nestjs/typeorm": "8.0.2",
    "axios": "0.21.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.1",
    "custom-env": "2.0.1",
    "dotenv": "8.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "4.17.21",
    "nest-router": "1.0.9",
    "nodemon": "2.0.7",
    "rxjs": "7.5.1",
    "typeorm": "0.2.36",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.0.0",
    "@nestjs/schematics": "8.0.0",
    "@nestjs/testing": "8.0.0",
    "@types/express": "4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "16.0.0",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "cross-env": "7.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "jest": "27.2.5",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
